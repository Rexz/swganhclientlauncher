<UserControl x:Class="ClientLauncher.Usercontrols.PatchProgress"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             >
    <Grid>
        <ScrollViewer x:Name="svProgress">
            <TextBlock x:Name="lblPatchProgress" FontFamily="{DynamicResource FontFamily}" FontSize="14" FontWeight="Bold" Foreground="{DynamicResource LightFill}" />
        </ScrollViewer>
    </Grid>
</UserControl>
